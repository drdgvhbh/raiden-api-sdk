{
  "name": "raiden-sdk",
  "version": "1.0.0",
  "description": "SDK for interacting with the Raiden API",
  "main": "lib/index.js",
  "typings": "dist/raiden-sdk.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "prepare": "npm run build",
    "build": "npm run clean && tsc && npm run extract",
    "docs": "typedoc --out ./docs ./src --excludePrivate --excludeProtected",
    "test": "jest",
    "extract": "api-extractor run --local",
    "lint": "eslint ./src/**/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drdgvhbh/raiden-node-sdk.git"
  },
  "keywords": [
    "payments",
    "raiden"
  ],
  "author": "Ryan Lee (drdgvhbh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drdgvhbh/raiden-node-sdk/issues"
  },
  "homepage": "https://github.com/drdgvhbh/raiden-node-sdk#readme",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.3.4",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.9",
    "@types/snakecase-keys": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.15.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "camelcase-keys": "^6.0.1",
    "dotenv": "^8.0.0",
    "raiden-swagger-sdk": "git@github.com:drdgvhbh/raiden-swagger-sdk.git#ed68144caa48204ea8310359bd7599bce4befdeb",
    "rxjs": "^6.5.2",
    "snakecase-keys": "^3.1.0",
    "xmlhttprequest": "^1.8.0"
  }
}
